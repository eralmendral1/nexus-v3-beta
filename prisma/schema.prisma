generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Client {
    id                  Int                 @id
    masterClient        Int?
    name                String
    email               String?
    disabled            Boolean             @default(false)
    address1            String?
    address2            String?
    city                String?
    state               String?
    zip                 String?
    phone1              String?
    phone2              String?
    fax                 String?
    createdAt           DateTime            @default(now())
    updatedAt           DateTime            @updatedAt
    policy_categories   PolicyCategory[]
    destination_numbers DestinationNumber[]
    orders              Order[]

    @@map("clients")
}

model DestinationNumber {
    id        Int      @id @default(autoincrement())
    client_id Int
    number    String
    extension String?
    label     String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    client    Client   @relation(fields: [client_id], references: [id])

    @@map("destination_numbers")
}

model PolicyCategory {
    id        Int      @id @default(autoincrement())
    name      String
    client_id Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    client    Client   @relation(fields: [client_id], references: [id])
    policies  Policy[]

    @@map("policy_categories")
}

model Policy {
    id          Int            @id @default(autoincrement())
    name        String
    category_id Int
    category    PolicyCategory @relation(fields: [category_id], references: [id])
    createdAt   DateTime       @default(now())
    updatedAt   DateTime       @updatedAt

    @@map("policies")
}

model User {
    id                      Int                @id @default(autoincrement())
    callcenter_id           Int?
    sid                     String?            @unique
    name                    String
    email                   String
    email_verified_at       DateTime?
    avatar                  String?
    password                String?
    admin                   Boolean            @default(false)
    external_agent          Boolean            @default(false)
    skills                  String?
    timezone                String?
    block_reject            Boolean            @default(true)
    presence                String?
    reservation_passthrough String?
    remember_token          String?
    createdAt               DateTime           @default(now())
    updatedAt               DateTime           @updatedAt
    deletedAt               DateTime?
    worker_capabilities     WorkerCapability[]
    trackers                UsersOnTrackers[]
    tracker_histories       TrackerHistory[]

    @@map("users")
}

model Item {
    id                     Int                @id @default(autoincrement())
    client_id              Int
    internal_name          String
    external_name          String
    self_init              Boolean            @default(false)
    channel_name           String
    channel_id             String
    self_init_workflow     String?
    workflow_id            String
    workflow_name          String?
    template               String
    icon_class             String?
    background_color       String?
    notification_tone      String?
    priority               Int?
    exclude                Boolean?
    timeout                Int?
    auto_dial              Boolean            @default(true)
    passthrough_url        String?
    pending_warning_limit  Int?
    pending_danger_limit   Int?
    accepted_warning_limit Int?
    accepted_danger_limit  Int?
    wrapup_time            Int?
    createdBy              String?
    createdAt              DateTime           @default(now())
    updatedAt              DateTime           @updatedAt
    deletedAt              DateTime?
    worker_capabilities    WorkerCapability[]
    order                  Order[]

    @@map("items")
}

model Contact {
    id        Int      @id @default(autoincrement())
    number    String
    notes     String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("contacts")
}

model WorkerCapability {
    id        Int      @id @default(autoincrement())
    worker_id Int
    item_id   Int
    status    String
    view_only Boolean  @default(false)
    BillType  String?
    BillRate  String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    user      User     @relation(fields: [worker_id], references: [id])
    item      Item     @relation(fields: [item_id], references: [id])

    @@map("worker_capabilities")
}

model Tracker {
    id          Int               @id @default(autoincrement())
    name        String
    description String?
    color       String?
    createdAt   DateTime          @default(now())
    updatedAt   DateTime          @updatedAt
    users       UsersOnTrackers[]

    @@map("trackers")
}

model TrackerHistory {
    id        Int      @id @default(autoincrement())
    user_id   Int
    type      String
    worker_id Int?
    meta_data String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    user      User     @relation(fields: [user_id], references: [id])

    @@map("tracker_histories")
}

model UsersOnTrackers {
    tracker    Tracker  @relation(fields: [tracker_id], references: [id])
    tracker_id Int
    user       User     @relation(fields: [user_id], references: [id])
    user_id    Int
    assignedAt DateTime @default(now())

    @@id([tracker_id, user_id])
    @@map("tracker_user")
}

model Order {
    id               Int               @id @default(autoincrement())
    item_id          Int
    client_id        Int
    status           String?
    scheduled_time   DateTime?
    target_worker    String?
    completed_at     DateTime?
    createdAt        DateTime          @default(now())
    updatedAt        DateTime          @updatedAt
    destination      String?
    DATA1            String?
    DATA2            String?
    DATA3            String?
    DATA4            String?
    DATA5            String?
    DATA6            String?
    DATA7            String?
    DATA8            String?
    DATA9            String?
    DATA10           String?
    DATA11           String?
    DATA12           String?
    DATA13           String?
    DATA14           String?
    DATA15           String?
    DATA16           String?
    DATA17           String?
    DATA18           String?
    DATA19           String?
    DATA20           String?
    DATA21           String?
    DATA22           String?
    DATA23           String?
    DATA24           String?
    DATA25           String?
    RDATA1           String?
    RDATA2           String?
    RDATA3           String?
    RDATA4           String?
    RDATA5           String?
    RDATA6           String?
    RDATA7           String?
    RDATA8           String?
    RDATA9           String?
    RDATA10          String?
    item             Item              @relation(fields: [item_id], references: [id])
    client           Client            @relation(fields: [client_id], references: [id])
    tasks            Task[]
    task_histories   TaskHistory[]
    task_completions TaskCompletions[]
    order_histories  OrderHistory[]

    @@map("orders")
}

model OrderHistory {
    id            Int       @id @default(autoincrement())
    order_id      Int
    status        String?
    target_worker String?
    batched       String?
    bached_id     String?
    last_update   DateTime?
    completed_at  DateTime?
    createdAt     DateTime  @default(now())
    updatedAt     DateTime  @updatedAt
    destination   String?
    DATA1         String?
    DATA2         String?
    DATA3         String?
    DATA4         String?
    DATA5         String?
    DATA6         String?
    DATA7         String?
    DATA8         String?
    DATA9         String?
    DATA10        String?
    DATA11        String?
    DATA12        String?
    DATA13        String?
    DATA14        String?
    DATA15        String?
    DATA16        String?
    DATA17        String?
    DATA18        String?
    DATA19        String?
    DATA20        String?
    DATA21        String?
    DATA22        String?
    DATA23        String?
    DATA24        String?
    DATA25        String?
    RDATA1        String?
    RDATA2        String?
    RDATA3        String?
    RDATA4        String?
    RDATA5        String?
    RDATA6        String?
    RDATA7        String?
    RDATA8        String?
    RDATA9        String?
    RDATA10       String?
    order         Order     @relation(fields: [order_id], references: [id])

    @@map("order_histories")
}

model Task {
    id                       Int               @id @default(autoincrement())
    order_id                 Int
    task_sid                 String            @unique
    task_priority            Int
    task_attributes          String?
    task_queue_target_exp    String
    task_queue_sid           String
    task_queue_name          String?
    task_channel_sid         String
    task_channel_unique_name String
    workflow_sid             String
    workflow_friendly_name   String
    workspace_sid            String
    workspace_name           String?
    account_sid              String
    resource_sid             String
    task_queue_entered_date  String
    task_created_date        String
    timestamp_ms             String?
    createdAt                DateTime          @default(now())
    updatedAt                DateTime          @updatedAt
    order                    Order             @relation(fields: [order_id], references: [id])
    task_histories           TaskHistory[]
    task_completions         TaskCompletions[]

    @@map("tasks")
}

model TaskHistory {
    id                     Int      @id @default(autoincrement())
    sid                    String
    order_id               Int
    item_id                Int
    event_type             String
    event_description      String
    task_sid               String
    task_priority          Int?
    task_attributes        String?
    task_assignment_status String
    task_age               Int
    task_version           String?
    reservation_sid        String?
    reservation_version    String?
    resource_type          String?
    resource_sid           String?
    worker_sid             String?
    worker_version         String?
    worker_activity_name   String?
    worker_name            String?
    worker_attributes      String?
    timestamp_ms           String?
    timestamp              String
    createdAt              DateTime @default(now())
    updatedAt              DateTime @updatedAt

    order Order @relation(fields: [order_id], references: [id])
    task  Task  @relation(fields: [task_sid], references: [task_sid])

    @@map("task_histories")
}

model TaskCompletions {
    id                Int       @id @default(autoincrement())
    order_id          Int
    item_id           Int
    task_sid          String
    worker_id         Int?
    worker_sid        String?
    final_outcome     String
    task_time         Int
    wait_time         Int
    talk_time         Int
    customer_messages Int?
    agent_messages    Int?
    total_messages    Int?
    BillType          String?
    BillPrice         String?
    BilLQty           String?
    task_created      DateTime
    task_assigned     DateTime?
    task_completed    DateTime?
    timestamp         String?
    createdAt         DateTime  @default(now())
    updatedAt         DateTime  @updatedAt
    order             Order     @relation(fields: [order_id], references: [id])
    task              Task      @relation(fields: [task_sid], references: [task_sid])

    @@map("task_completions")
}
